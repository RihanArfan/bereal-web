{
  "name": "bereal",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:frontend": "vite",
    "dev:api": "wrangler pages dev --proxy 5173 --compatibility-date=2023-01-08 --log-level warn",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@tanstack/query-persist-client-core": "^4.22.0",
    "@tanstack/query-sync-storage-persister": "^4.22.0",
    "@tanstack/vue-query": "^4.22.0",
    "@vueuse/components": "^9.10.0",
    "@vueuse/core": "^9.10.0",
    "dayjs": "^1.11.7",
    "jwt-decode": "^3.1.2",
    "ky": "^0.33.2",
    "pinia": "^2.0.28",
    "tailwindcss": "^3.2.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tanstack/eslint-plugin-query": "^4.21.0",
    "@tanstack/query-core": "^4.22.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.24",
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4",
    "wrangler": "^2.7.1"
  },
  "peerDependencies": {
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4"
  }
}
